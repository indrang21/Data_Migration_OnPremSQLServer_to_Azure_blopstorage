{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "practice212121"
		},
		"AzureBlobStorage_sink1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_sink1'"
		},
		"AzureDataLakeStorage_sink1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_sink1'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer_source1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_source1'"
		},
		"AzureDataLakeStorage_sink1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakepractice2121.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "practice212121_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_sink1_connectionString": {
						"value": "[parameters('AzureBlobStorage_sink1_connectionString')]"
					},
					"AzureDataLakeStorage_sink1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_sink1_accountKey')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServer_source1_connectionString": {
						"value": "[parameters('SqlServer_source1_connectionString')]"
					},
					"AzureDataLakeStorage_sink1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_sink1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}